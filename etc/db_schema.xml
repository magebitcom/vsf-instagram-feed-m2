<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="magebit_instagramfeed_post" resource="default" engine="innodb" comment="magebit_instagramfeed_post Table">
		<column xsi:type="smallint" name="post_id" padding="6" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="post_id"/>
		</constraint>
		<column name="media" nullable="true" xsi:type="text"/>
		<column name="caption" nullable="true" xsi:type="text"/>
		<column name="permalink" nullable="true" xsi:type="text"/>
		<column name="like_count" nullable="true" xsi:type="int" identity="false"/>
		<column name="id" nullable="true" xsi:type="text"/>
		<column name="media_hq" nullable="true" xsi:type="text"/>
		<column name="store_id" nullable="true" xsi:type="text"/>
	</table>
</schema>
